<body>
  <!-- header -->
  <div class="container p-0">
  <header class="d-lg-flex justify-content-lg-around" style="padding-top: 20px;">
    <div class="container d-lg-none p-0">
      <a href="/views/index.html" class="d-flex justify-content-center flex-grow-1"><img src="/images/logo.png"
          class="img-fluid logo" alt="logo">
        <!-- <img src=/images/logo.png" class="img-fluid" alt="logo"> -->
      </a>
    </div><br>
    <nav class="navbar navbar-dark bg-dark flex-lg-fill">
      <!-- logo lg -->
      <div id="logoLG" class="d-lg-flex order-lg-1 d-none d-xs-none d-sm-none d-md-none">
        <a href="/views/index.html"><img src="/images/logo.png" class="img-fluid logo" alt="logo">
          <!-- <img src=/images/logo.png" class="img-fluid" alt="logo"> -->
        </a>
      </div>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#categorias"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <form class="form-inline my-2 my-lg-0 order-lg-1 flex-md-grow-1 justify-content-center">
        <input class="form mr-sm-1 p-0 navSearch" type="search" placeholder=" Buscar" aria-label="Search">
        <button class="btn btn-dark my-2 my-sm-0" type="submit" style="border-color: #6c757d;"><i class="fas fa-search"
            style="color: white;"></i></button>
      </form>
      <!-- user -->
      <div class="dropdown order-lg-1">
        <a class="" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" data-display="static"
          aria-haspopup="true" aria-expanded="false">
          <button type="button" class="btn btn-secondary bg-dark my-1 my-lg-0"><i class="far fa-user"></i></button>
        </a>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
          <a class="dropdown-item" href="#">Entrar</a>
          <a class="dropdown-item" href="#">Registrarse</a>
        </div>
      </div>
      <button type="button" class="btn btn-secondary bg-dark my-2 my-lg-0 order-lg-1"><i
          class="fas fa-shopping-cart"></i></button>
      <div class="collapse navbar-collapse  order-lg-1" id="categorias">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="#">Accesorios</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Bajos</a>
